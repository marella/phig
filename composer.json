{
    "name": "marella/phig",
    "description": "PHP Config library.",
    "type": "library",
    "keywords": ["config", "library", "phig", "php", "json", "ini", "xml", "yaml", "yml"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/marella/phig/issues",
        "source": "https://github.com/marella/phig"
    },
    "authors": [
        {
            "name": "Ravindra Marella",
            "email": "mv.ravindra007@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11",
        "mockery/mockery": "~0.9.2",
        "phpunit/phpunit": "4.8.*",
        "phpunit/php-code-coverage": "^2",
        "satooshi/php-coveralls": "^1.0",
        "symfony/yaml": "^3.0"
    },
    "suggest": {
        "symfony/yaml": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phig\\": "src/Phig/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phig\\Tests\\": "tests/"
        }
    }
}
